<p id="notice"><%= notice %></p>

<h1>Listing Course Enrollments</h1>

<table>
  <thead>
    <tr>
      <th>Presence count</th>
      <th>Class activity score</th>
      <th>Mid term score</th>
      <th>Final term score</th>
      <th>Final score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_enrollments.each do |course_enrollment| %>
      <tr>
        <td><%= course_enrollment.presence_count %></td>
        <td><%= course_enrollment.class_activity_score %></td>
        <td><%= course_enrollment.mid_term_score %></td>
        <td><%= course_enrollment.final_term_score %></td>
        <td><%= course_enrollment.final_score %></td>
        <td><%= link_to 'Show', course_enrollment %></td>
        <td><%= link_to 'Edit', edit_course_enrollment_path(course_enrollment) %></td>
        <td><%= link_to 'Destroy', course_enrollment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course enrollment', new_course_enrollment_path %>
